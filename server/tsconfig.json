{
    "compilerOptions": {
      "baseUrl": ".",
      "outDir": "./",
      "module": "esnext",
      "target": "es5",
      "lib": ["es6"],
      "sourceMap": true,
      "moduleResolution": "node",
      "rootDir": "./",
      "forceConsistentCasingInFileNames": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "suppressImplicitAnyIndexErrors": true,
      "noUnusedLocals": true
    },
    "exclude": [
      "../node_modules"
    ]
  }
  